{
  "name": "server",
  "version": "1.0.1",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "build": "tsc",
    "prestart": "pnpm build",
    "start": "pnpm build && pnpm migrate && cross-env NODE_ENV=production ts-node dist/index.js",
    "postinstall": "prisma generate --schema=./prisma/schema.prisma",
    "migrate": "prisma migrate dev",
    "dev": "cross-env NODE_ENV=development ts-node-dev index.ts",
    "test": "cross-env NODE_ENV=development tslint -c tslint.json -p tsconfig.json ; mocha -r ts-node/register --recursive 'test/**/*.spec.ts'"
  },
  "dependencies": {
    "@prisma/client": "^2.22.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "celebrate": "^12.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "http-terminator": "^2.0.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/http-terminator": "^2.0.1",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.23",
    "@types/sinon": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.26.0",
    "mocha": "^8.0.1",
    "prisma": "^2.22.1",
    "sinon": "^9.0.2",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.51",
    "typescript": "^3.9.6"
  }
}
